{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Error",
        "Microsoft.EntityFrameworkCore": "Fatal",
        "Microsoft.EntityFrameworkCore.Database.Command": "Fatal"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "..\\Logs\\log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}"
        }
      }
    ]
  },
  "HangfireVariables": {
    "User": "admin",
    "Pass": "Asdf@1234",
    "Title": "Hangfire Job",
    "DisplayConnectionString": false,
    "TimeZone": "Asia/Dhaka"
  },
  "TotpSettings": {
    "Issuer": "OE Authenticator"
  }
}